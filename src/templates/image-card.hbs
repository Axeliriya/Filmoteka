{{#each this}}
<li class="picture__item" data-id={{id}} data-largeimg={{largeImageURL}}>
    <div class="photo-card">
        <a class="thumb js-thumb" href={{largeImageURL}}>
            <img class="found-image" src={{webformatURL}} data-largeimg={{largeImageURL}} data-likes={{likes}}
                data-views={{views}} data-comments={{comments}} data-downloads={{downloads}} data-id={{id}} />
        </a>

        <div class="stats">
            <p class="stats-item">
                <i class="material-icons">thumb_up</i>
                {{likes}}
            </p>
            <p class="stats-item">
                <i class="material-icons">visibility</i>
                {{views}}
            </p>
            <p class="stats-item">
                <i class="material-icons">comment</i>
                {{comments}}
            </p>
            <p class="stats-item">
                <i class="material-icons">cloud_download</i>
                {{downloads}}
            </p>
        </div>
    </div>
</li>
{{/each}}